// numeromayor.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

int main (){
	int n,i=0,a,mayor=-99999;
	cout << "ingrese la cantidad de numeros: ";
	cin >> n;
	for (i=1; i<=n; i++){
        cout << "ingrese el numero: ";
		cin >>a;
		if (mayor<a){
	       mayor=a;
	 }
  }
  cout << "el numero mayor es: " << a << endl;
  getch();
  return 0;
}
